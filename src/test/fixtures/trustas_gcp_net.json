{
    "name": "sample-network",
    "description": "Minimal network for TrustAS for GCP execution",
    "version": "0.1",
    "client": {
        "organization": "Org1",
        "credentialStore": {
            "path": "/tmp/hfc-kvs",
            "cryptoStore": {
                "path": "/tmp/hfc-cvs"
            },
            "wallet": "wallet-name"
        }
    },
    "organizations": {
        "orderer.example.com": {
            "mspid": "OrdererMSP",
            "orderers": [
                "orderer.example.com"
            ],
            "certificateAuthorities": [
                "ca-orderer"
            ],
            "users": {
                "Admin": {
                    "cert": "test/fixtures/e2e_cli/crypto-config/ordererOrganizations/example.com/users/Admin@example.com/msp/signcerts/Admin@example.com-cert.pem",
                    "private_key": "test/fixtures/e2e_cli/crypto-config/ordererOrganizations/example.com/users/Admin@example.com/msp/keystore/aca9d29a0056b2baab8692ed75e2487a38c84e6db86e651ec27cb7f97a8fdd9e_sk"
                }
            }
        },
        "org1.example.com": {
            "mspid": "Org1MSP",
            "peers": [
                "as000.org1.example.com",
                "as001.org1.example.com"
            ],
            "certificateAuthorities": [
                "ca-org1"
            ],
            "users": {
                "Admin": {
                    "cert": "test/fixtures/e2e_cli/crypto-config/peerOrganizations/org1.example.com/users/Admin@org1.example.com/msp/signcerts/Admin@org1.example.com-cert.pem",
                    "private_key": "test/fixtures/e2e_cli/crypto-config/peerOrganizations/org1.example.com/users/Admin@org1.example.com/msp/keystore/864f60da9c0aafa778658030a69ab4a844e305768cb65b77db05aedb8cbcc16d_sk"
                },
                "User1": {
                    "cert": "test/fixtures/e2e_cli/crypto-config/peerOrganizations/org1.example.com/users/User1@org1.example.com/msp/signcerts/User1@org1.example.com-cert.pem",
                    "private_key": "test/fixtures/e2e_cli/crypto-config/peerOrganizations/org1.example.com/users/User1@org1.example.com/msp/keystore/a025dafac4b23f65d4529a3a37609edb3b99d90b0ceb250e4a65aeebe81aa84f_sk"
                }
            }
        }
    },
    "orderers": {
        "orderer.example.com": {
            "url": "localhost:7050",
            "grpcOptions": {
                "ssl-target-name-override": "orderer.example.com",
                "grpc-max-send-message-length": 15
            },
            "tlsCACerts": {
                "path": "test/fixtures/e2e_cli/crypto-config/ordererOrganizations/example.com/tlsca/tlsca.example.com-cert.pem"
            }
        }
    },
    "peers": {
        "as000.org1.example.com": {
            "url": "localhost:7051",
            "eventUrl": "localhost:7053",
            "grpcOptions": {
                "ssl-target-name-override": "as000.org1.example.com",
                "grpc.http2.keepalive_time": 15
            },
            "tlsCACerts": {
                "path": "test/fixtures/e2e_cli/crypto-config/peerOrganizations/org1.example.com/peers/as000.org1.example.com/msp/tlscacerts/tlsca.org1.example.com-cert.pem"
            }
        },
        "as001.org1.example.com": {
            "url": "localhost:8051",
            "eventUrl": "localhost:8053",
            "grpcOptions": {
                "ssl-target-name-override": "as001.org1.example.com"
            },
            "tlsCACerts": {
                "path": "test/fixtures/e2e_cli/crypto-config/peerOrganizations/org1.example.com/peers/as001.org1.example.com/msp/tlscacerts/tlsca.org1.example.com-cert.pem"
            }
        }
    },
    "certificateAuthorities": {
        "ca-org1": {
            "url": "https://localhost:7054",
            "grpcOptions": {
                "verify": true
            },
            "tlsCACerts": {
                "path": "test/fixtures/e2e_cli/crypto-config/peerOrganizations/org1.example.com/ca/org1.example.com-cert.pem"
            },
            "registrar": [{
                "enrollId": "admin",
                "enrollSecret": "adminpw"
            }]
        }
    }
